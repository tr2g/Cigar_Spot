<div class='container'>
  <h5>ジャンル名編集</h5>
  <div class='col-7 mt-5'>
    <%= form_with model: @genre, url: admin_genre_path, method: :patch do |f| %>
    <strong class="mr-5">ジャンル名</strong>
      <%= f.text_field :name, class: "mr-5" %>
      <%= f.submit "変更", class: "btn btn-success" %>
    <% end %>
  </div>
</div>